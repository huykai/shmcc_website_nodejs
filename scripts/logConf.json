{
  "appenders":
  {
    "console": {
      "type": "console"
    },
    "default": {
      "type": "dateFile",
      "filename": "log_file/file.log",
      "pattern": "-yyyy-MM-dd.log",
      "alwaysIncludePattern": true,
      "maxLogSize": 104800,
      "backups": 100
    }
  },
  "categories": {
      "default": {
          "appenders": ["default"], 
          "level": "info" 
      },
      "log_console_file": {
          "appenders": ["console","default"], 
          "level": "info" 
      },
      "log_file": {
          "appenders": ["default"], 
          "level": "info" 
      }
  },
  "replaceConsole": false
}
